// lista.h
#include <stdio.h>
typedef struct  {
  int chave;
} tipoitem;

typedef struct tipono {
  tipoitem item;
  struct tipono * prox;
}tipono;

typedef struct tipono * apontador;

typedef struct {
  apontador inicio;
  apontador fim;
} tipolista;

void inicia(tipolista * lista);
int vazia (tipolista lista);
